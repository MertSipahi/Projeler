import java.util.Scanner;
public class usluSayi {
    public static void main(String[] args) {
        int k,l,m,n=1;
        Scanner scan = new Scanner(System.in);
        System.out.println("sayı giriniz");
        k=scan.nextInt();
        System.out.println("sayı giriniz");
        l=scan.nextInt();
        for (m = 0; m <= l; m++) {
            n*=k;
        }
        System.out.println("sayının üstü: " + n);
    }
}
