import java.util.Scanner;
public class basSayiTop {
    public static void main(String[] args) {
        Scanner scan= new Scanner(System.in);
        int k,m=0,n=0;
        System.out.println("sayÄ± giriniz: ");
        k= scan.nextInt();

        while (k != 0) {
            m = k%10;
            n+=m;
            k /= 10;
        }
        System.out.println(n);
    }
}
